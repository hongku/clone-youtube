extends layouts/main
include mixins/videoBlock

block content
  .search__header
    h3 Searching for: #{searchingBy}
  .search__videos
    if videos.length === 0
      h5 Not Found Videos
    each video in videos
      +videoBlock({
        id: video.id,
        title: video.title,
        views: video.views,
        videoFile: video.fileUrl,
      })